   "source": [
    "def squarecubes(arr):\n",
    "    a = []\n",
    "    b = []\n",
    "    for x in arr:\n",
    "        a.append(x*x)\n",
    "        b.append(x*x*x)\n",
    "    return tuple([a, b])\n",
    "\n",
    "x = [2,3,5]\n",
    "\n",
    "print(squarecubes(x))"
   ]
